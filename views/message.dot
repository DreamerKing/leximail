<article id="post">
	<header class="post-link">
		<div class="details">
			<!-- img class="icon" width="16" height="16" src="img/gmail.png" -->
			<span class="feed">Google Account</span> | 
			<span class="time">{{=it.date}}</span>
		</div>
		<div class="emails">
			{{~it.from :v:i}}
			From: <a href="#" class="account">{{=v.name}} &lt;{{=v.address}}&gt;</a>
			{{~}}
		</div>
		<h1 class="title">{{=it.subject}}</h1>
	</header>
	<iframe class="body"></iframe>
</article>
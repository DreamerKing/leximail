<li class="article" data-id="{{=it.id}}">
	<header>
		<img class="icon" width="16" height="16" src="themes/default/img/gmail.png">
		<span class="feed">Google Account</span>
		<span class="time">{{=it.date}}</span>
	</header>
	<span class="title">{{=it.subject}}</span>
	<span class="snippet">{{=it.snipet}}</span>
</li>